<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/onboarding_dialog_button_cancel_pressed"/>
            <corners android:radius="@dimen/onboarding_button_selector_corner_radius"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/onboarding_dialog_button_cancel_normal"/>
            <corners android:radius="@dimen/onboarding_button_selector_corner_radius"/>
        </shape>
    </item>
</selector>
